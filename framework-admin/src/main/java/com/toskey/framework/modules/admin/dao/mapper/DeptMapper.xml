<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toskey.framework.modules.admin.dao.DeptDao">

    <select id="getDeptUser" resultType="string">
        SELECT id FROM sys_user WHERE dept_id LIKE CONCAT('%', #{deptId}, ',')
    </select>


</mapper>